{
  "name": "streetpaws",
  "version": "1.0.0",
  "description": "StreetPaws - Promoting Safe and Compassionate Communities",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:user": "vite --config vite.user.config.ts",
    "dev:admin": "vite --config vite.admin.config.ts",
    "build:user": "vite build --config vite.user.config.ts",
    "build:admin": "vite build --config vite.admin.config.ts",
    "preview:user": "vite preview --config vite.user.config.ts",
    "preview:admin": "vite preview --config vite.admin.config.ts",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "cleanup-animals": "node cleanup-and-restore-animals.js",
    "migrate-reports": "node scripts/migrate-reports.js",
    "migrate-reports:cleanup": "node scripts/migrate-reports.js --cleanup",
    "migrate-lost-found-to-reports": "node scripts/migrate-lost-found-to-reports.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@types/leaflet": "^1.9.20",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "firebase": "^10.7.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-hot-toast": "^2.4.1",
    "react-leaflet": "^4.2.1",
    "leaflet.heat": "^0.2.0",
    "react-router-dom": "^6.8.1",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "vite": "^5.4.19"
  }
}
